{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/import/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/process-research/?$",
			"dest": "_render"
		},
		{
			"src": "^/concepts(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/faq/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/updates/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}